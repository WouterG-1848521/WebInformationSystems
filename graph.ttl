@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix local: <http://localhost/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


# beroepen

local:profession/doctor rdf:type owl:Class ;
    rdfs:subClassOf local:profession .

local:profession/Chirurg rdf:type owl:Class ;
    rdfs:subClassOf local:profession/doctor .

local:profession/familyDoctor rdf:type owl:Class ;
    rdfs:subClassOf local:profession/doctor .

local:profession/teacher rdf:type owl:Class ;
    rdfs:subClassOf local:profession .

local:profession/programmer rdf:type owl:Class ;
    rdfs:subClassOf local:profession .

# degrees

local:degree/master rdf:type owl:Class ;
    rdfs:subClassOf local:degree .

local:degree/bachelor rdf:type owl:Class ;
    rdfs:subClassOf local:degree .

# enterprise

local:enterprise rdf:type owl:Class ;
    rdfs:subClassOf foaf:Organization .

# enterpriseInfo

local:enterpriseInfo rdf:type owl:Class .

# vacancy

local:Vacancy rdf:type owl:Class .
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:vacancyInfo;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] .
        # elke vacature heeft min en max 1 vacatureinfo

# vacancyInfo

local:VacancyInfo rdf:type owl:Class .

# personalInfo

local:personalInfo rdf:type owl:Class ;
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:email;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] ;
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:phone;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] .
    #  elke personalInfo heeft min en max 1 email, phone

# Person

foaf:person owl:subClassOf [
    rdf:type owl:Restriction;
    owl:onProperty local:name;
    owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] ;
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:surname;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] ;
    owl:subClassOf [
        rdf:type owl:personalInfo;
        owl:onProperty local:surname;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger
    ] .

# elke person heeft min en max 1 name, surname, personalInfo
# TODO : elke person heeft min en max 1 connections bag, desnoods leeg

# ConnectionRequests
local:ConnectionRequest rdf:type owl:Class ;
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:sender;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger 
    ] ;
    owl:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty local:receiver;
        owl:cardinality ”1”ˆˆxsd:nonNegativeInteger 
    ] .
#  elke request heeft min en max 2 personen, x != y



